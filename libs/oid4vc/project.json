{
  "name": "oid4vc",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/oid4vc/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/oid4vc",
        "main": "libs/oid4vc/src/index.ts",
        "tsConfig": "libs/oid4vc/tsconfig.lib.json",
        "assets": ["libs/oid4vc/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/oid4vc/jest.config.ts"
      }
    }
  },
  "tags": []
}
